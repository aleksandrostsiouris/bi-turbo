{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true,
    },
    "transform": {},
    "target": "es2022",
    "loose": false,
    "externalHelpers": false,
    // Requires v1.2.50 or upper and requires target to be es2016 or upper.
    "keepClassNames": false,
    "preserveAllComments": false
  },
  "module": {
    "type": "commonjs"
  },
  "minify": false,
  "sourceMaps": true
}